   "id": "915e4151",
   "metadata": {},
   "outputs": [
    {
     "data": {
      "text/plain": [
       "Date    datetime64[ns]\n",
       "eth            float64\n",
       "btc            float64\n",
       "bnb            float64\n",
       "dtype: object"
      ]
     },
     "execution_count": 73,
     "metadata": {},
     "output_type": "execute_result"
    }
   ],
   "source": [
    "eth = pd.read_csv('ethereum.csv', usecols=[\"Date\",\"Close\"])\n",
    "btc = pd.read_csv('bitcoin.csv', usecols=[\"Date\",\"Close\"])\n",
    "bnb = pd.read_csv('BNB.csv', usecols=[\"Date\",\"Close\"])\n",
    "\n",
    "eth.rename(columns = {'Close': \"eth\"}, inplace = True)\n",
    "btc.rename(columns = {'Close': \"btc\"}, inplace = True)\n",
    "bnb.rename(columns = {'Close': \"bnb\"}, inplace = True)\n",
    "\n",
    "df = pd.merge(pd.merge(eth, btc, on='Date'),bnb, on='Date')\n",
    "df[\"Date\"] = pd.to_datetime(df[\"Date\"])\n",
    "df.dtypes"
   ]
  },
